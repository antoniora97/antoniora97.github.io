---

<script>
  import { filter, range } from "rxjs";

  const observable = range(1, 10);
  const observablePares = observable.pipe(filter((x) => x % 2 === 0));

  console.log("Observable original");
  observable.subscribe((x) => console.log(x));

  console.log("");

  console.log("Observable Pares");
  observablePares.subscribe((x) => console.log(x));
</script>
